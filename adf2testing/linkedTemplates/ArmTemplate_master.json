			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer1_onprimese_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1_onprimese'"
		},
		"cdc_onprimese_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cdc_onprimese'"
		},
		"linked_service_adl_accountKey": {
			"type": "secureString",
